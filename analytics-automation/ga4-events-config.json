{
  "ga4Configuration": {
    "propertyId": "4NZR3HR3J1",
    "propertyName": "SoundFlows - Main",
    "measurementId": "G-4NZR3HR3J1",
    "dataRetention": "14_months",
    "enhancedMeasurement": true,
    "ipAnonymization": true,
    "adPersonalization": false
  },
  "customEvents": [
    {
      "eventName": "audio_play",
      "category": "audio",
      "description": "Track when audio track is played",
      "parameters": {
        "audio_category": "dimension1",
        "audio_format": "dimension2",
        "device_type": "dimension3"
      },
      "conversion": true,
      "conversionValue": 1
    },
    {
      "eventName": "audio_duration",
      "category": "engagement",
      "description": "Track audio listening duration",
      "parameters": {
        "duration_seconds": "value",
        "track_name": "event_label"
      },
      "conversion": false
    },
    {
      "eventName": "featured_track_play",
      "category": "feature",
      "description": "Track featured track plays",
      "parameters": {
        "track_name": "event_label"
      },
      "conversion": true,
      "conversionValue": 2
    },
    {
      "eventName": "category_switch",
      "category": "navigation",
      "description": "Track category navigation changes",
      "parameters": {
        "category_name": "event_label"
      },
      "conversion": false
    },
    {
      "eventName": "theme_change",
      "category": "feature",
      "description": "Track theme changes",
      "parameters": {
        "theme_name": "event_label"
      },
      "conversion": false
    },
    {
      "eventName": "language_change",
      "category": "feature",
      "description": "Track language changes",
      "parameters": {
        "language_code": "event_label"
      },
      "conversion": false
    },
    {
      "eventName": "share",
      "category": "social",
      "description": "Track content sharing",
      "parameters": {
        "platform": "event_label"
      },
      "conversion": true,
      "conversionValue": 3
    },
    {
      "eventName": "tutorial_complete",
      "category": "onboarding",
      "description": "Track tutorial completion",
      "parameters": {
        "tutorial_type": "event_label"
      },
      "conversion": true,
      "conversionValue": 5
    },
    {
      "eventName": "sleep_timer_set",
      "category": "feature",
      "description": "Track sleep timer usage",
      "parameters": {
        "timer_minutes": "value"
      },
      "conversion": false
    },
    {
      "eventName": "scroll_depth",
      "category": "engagement",
      "description": "Track page scroll depth",
      "parameters": {
        "scroll_percentage": "value"
      },
      "conversion": false
    },
    {
      "eventName": "audio_error",
      "category": "error",
      "description": "Track audio playback errors",
      "parameters": {
        "error_type": "event_label"
      },
      "conversion": false
    },
    {
      "eventName": "javascript_error",
      "category": "error",
      "description": "Track JavaScript errors",
      "parameters": {
        "error_location": "event_label"
      },
      "conversion": false
    },
    {
      "eventName": "outbound_click",
      "category": "navigation",
      "description": "Track outbound link clicks",
      "parameters": {
        "destination_domain": "event_label"
      },
      "conversion": false
    }
  ],
  "conversions": [
    {
      "eventName": "audio_play",
      "conversionName": "Audio Play",
      "countingMethod": "once_per_event",
      "conversionValue": 1,
      "currencyCode": "USD"
    },
    {
      "eventName": "featured_track_play",
      "conversionName": "Featured Track Play",
      "countingMethod": "once_per_event",
      "conversionValue": 2,
      "currencyCode": "USD"
    },
    {
      "eventName": "share",
      "conversionName": "Content Share",
      "countingMethod": "once_per_event",
      "conversionValue": 3,
      "currencyCode": "USD"
    },
    {
      "eventName": "tutorial_complete",
      "conversionName": "Tutorial Completion",
      "countingMethod": "once_per_session",
      "conversionValue": 5,
      "currencyCode": "USD"
    }
  ],
  "customDimensions": [
    {
      "dimensionName": "Audio Category",
      "dimensionId": "dimension1",
      "scope": "event",
      "description": "Category of audio being played"
    },
    {
      "dimensionName": "Audio Format",
      "dimensionId": "dimension2",
      "scope": "event",
      "description": "Format of audio file (MP3, WAV, etc.)"
    },
    {
      "dimensionName": "Device Type",
      "dimensionId": "dimension3",
      "scope": "event",
      "description": "Type of device being used"
    },
    {
      "dimensionName": "User Type",
      "dimensionId": "dimension4",
      "scope": "user",
      "description": "Type of user (new, returning, etc.)"
    }
  ],
  "customMetrics": [
    {
      "metricName": "Audio Duration Seconds",
      "metricId": "metric1",
      "scope": "event",
      "description": "Duration of audio playback in seconds"
    },
    {
      "metricName": "Scroll Percentage",
      "metricId": "metric2",
      "scope": "event",
      "description": "Scroll depth percentage"
    }
  ],
  "audiences": [
    {
      "audienceName": "Highly Engaged Users",
      "description": "Users who have played audio multiple times",
      "membershipDurationDays": 30,
      "inclusionCriteria": {
        "and": [
          {
            "eventFilter": {
              "eventName": "audio_play",
              "comparisonType": "greater_than_or_equal",
              "comparisonValue": 3
            }
          },
          {
            "sessionFilter": {
              "metricName": "session_duration",
              "comparisonType": "greater_than",
              "comparisonValue": 300
            }
          }
        ]
      }
    },
    {
      "audienceName": "Featured Track Enthusiasts",
      "description": "Users who frequently play featured tracks",
      "membershipDurationDays": 14,
      "inclusionCriteria": {
        "eventFilter": {
          "eventName": "featured_track_play",
          "comparisonType": "greater_than_or_equal",
          "comparisonValue": 2
        }
      }
    },
    {
      "audienceName": "Social Sharers",
      "description": "Users who have shared content",
      "membershipDurationDays": 90,
      "inclusionCriteria": {
        "eventFilter": {
          "eventName": "share",
          "comparisonType": "greater_than_or_equal",
          "comparisonValue": 1
        }
      }
    },
    {
      "audienceName": "Power Listeners",
      "description": "Users with long listening sessions",
      "membershipDurationDays": 30,
      "inclusionCriteria": {
        "and": [
          {
            "eventFilter": {
              "eventName": "audio_duration",
              "comparisonType": "greater_than",
              "comparisonValue": 600
            }
          },
          {
            "userPropertyFilter": {
              "userPropertyName": "total_session_count",
              "comparisonType": "greater_than_or_equal",
              "comparisonValue": 5
            }
          }
        ]
      }
    },
    {
      "audienceName": "New User Explorers",
      "description": "New users who completed tutorial",
      "membershipDurationDays": 7,
      "inclusionCriteria": {
        "and": [
          {
            "userPropertyFilter": {
              "userPropertyName": "first_session_timestamp",
              "comparisonType": "greater_than",
              "comparisonValue": "7_days_ago"
            }
          },
          {
            "eventFilter": {
              "eventName": "tutorial_complete",
              "comparisonType": "greater_than_or_equal",
              "comparisonValue": 1
            }
          }
        ]
      }
    },
    {
      "audienceName": "Mobile Users",
      "description": "Users accessing from mobile devices",
      "membershipDurationDays": 30,
      "inclusionCriteria": {
        "deviceCategoryFilter": {
          "deviceCategory": "mobile"
        }
      }
    },
    {
      "audienceName": "Desktop Users",
      "description": "Users accessing from desktop devices",
      "membershipDurationDays": 30,
      "inclusionCriteria": {
        "deviceCategoryFilter": {
          "deviceCategory": "desktop"
        }
      }
    },
    {
      "audienceName": "Deep Scrollers",
      "description": "Users who scroll 90%+ of pages",
      "membershipDurationDays": 14,
      "inclusionCriteria": {
        "eventFilter": {
          "eventName": "scroll_depth",
          "comparisonType": "greater_than_or_equal",
          "comparisonValue": 90
        }
      }
    }
  ],
  "goals": [
    {
      "goalName": "Audio Engagement Goal",
      "description": "Users who play audio for more than 30 seconds",
      "type": "event",
      "eventDetails": {
        "eventName": "audio_duration",
        "condition": "greater_than_or_equal",
        "value": 30
      },
      "value": 1
    },
    {
      "goalName": "Feature Adoption Goal",
      "description": "Users who complete tutorial",
      "type": "event",
      "eventDetails": {
        "eventName": "tutorial_complete",
        "condition": "greater_than_or_equal",
        "value": 1
      },
      "value": 5
    },
    {
      "goalName": "Social Sharing Goal",
      "description": "Users who share content",
      "type": "event",
      "eventDetails": {
        "eventName": "share",
        "condition": "greater_than_or_equal",
        "value": 1
      },
      "value": 3
    },
    {
      "goalName": "Content Exploration Goal",
      "description": "Users who explore multiple categories",
      "type": "event",
      "eventDetails": {
        "eventName": "category_switch",
        "condition": "greater_than_or_equal",
        "value": 3
      },
      "value": 2
    }
  ],
  "reports": [
    {
      "reportName": "Audio Performance Dashboard",
      "description": "Comprehensive audio performance metrics",
      "dimensions": ["audio_category", "device_type"],
      "metrics": ["audio_play_count", "avg_audio_duration", "featured_track_play_count"],
      "filters": []
    },
    {
      "reportName": "User Engagement Report",
      "description": "User behavior and engagement metrics",
      "dimensions": ["device_category", "user_type"],
      "metrics": ["session_duration", "page_views_per_session", "bounce_rate"],
      "filters": []
    },
    {
      "reportName": "Feature Adoption Analysis",
      "description": "Feature usage and adoption rates",
      "dimensions": ["feature_name", "user_type"],
      "metrics": ["feature_usage_count", "conversion_rate"],
      "filters": []
    }
  ],
  "alerts": [
    {
      "alertName": "Audio Error Rate Spike",
      "description": "Alert when audio error rate exceeds 5%",
      "condition": {
        "metric": "audio_error_rate",
        "operator": "greater_than",
        "threshold": 5,
        "duration": "1_hour"
      },
      "notificationMethod": "email"
    },
    {
      "alertName": "Traffic Drop Alert",
      "description": "Alert when daily traffic drops by 30%",
      "condition": {
        "metric": "active_users",
        "operator": "decrease_by_percent",
        "threshold": 30,
        "duration": "1_day"
      },
      "notificationMethod": "email"
    },
    {
      "alertName": "Featured Track Performance",
      "description": "Alert when featured track plays increase by 50%",
      "condition": {
        "metric": "featured_track_play_count",
        "operator": "increase_by_percent",
        "threshold": 50,
        "duration": "1_week"
      },
      "notificationMethod": "email"
    }
  ]
}